<?xml version="1.0"?>
<skin version="1.0" name="fsjw25" author="FlashSkins">

	<settings>
		<setting name="backcolor" value="0x6622BB" />
		<setting name="frontcolor" value="0x666666" />
		<setting name="lightcolor" value="0x000000" />
		<setting name="screencolor" value="0x292929" />
	</settings>

	<components>
		<component name="controlbar">
			<settings>
				<setting name="backgroundcolor" value="0x191919"/>
				<setting name="margin" value="2" />
				<setting name="fontcolor" value="0x848484" />
				<setting name="fontsize" value="10" />
			</settings>

<layout>
   <group position="left">
      <button name="play" />
      <button name="stop" />
     <text name="elapsed" />
   </group>
   
   <group position="center">
      <slider name="time" />
   </group>
   
   <group position="right">
      <text name="duration" />
      <button name="fullscreen" />
      <button name="mute" />
      <divider width="6" />
      <slider name="volume" />
   </group>
</layout>

			<elements>
				<element name="background" src="background.png" />
